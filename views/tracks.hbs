<div class="tracks">
    <ol>
        {{#each tracks}}
        <li>
            <div class="track">
                <p>{{this.name}}</p>
                <audio controls src="{{this.preview_url}}"> Your browser does not support the <code>audio</code> element. 
                </audio>
            </div>
        </li>
        {{/each}}
    </ol>    
</div>
